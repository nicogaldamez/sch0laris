Utils.loading(false)

<% if @page == 1 %>
	$("#questions_list").html("<%= escape_javascript(render('questions_list', collection:@questions))%>");
  Utils.activeMenuItem('filter_nav', '<%= @tab %>');
<% else %>
	$("#questions_list").append("<%= escape_javascript(render('questions_list', collection:@questions))%>");  
<% end %>

$('.pagination').replaceWith('<%= j will_paginate(@questions) %>');

<% if @questions.next_page %>
  $('.pagination').replaceWith('<%= j will_paginate(@questions) %>');
<% else %>
  $('.pagination').html('');
<% end %>

