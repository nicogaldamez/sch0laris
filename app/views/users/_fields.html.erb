<%= render 'shared/error_messages', object: f.object %>

<%= f.label :name, "Nombre" %>
<%= f.text_field :name %>

<%= f.label :email, "Correo electrónico" %>
<%= f.text_field :email %>

<%= label_tag :dateOfBirth, "Fecha de Nacimiento" %>
<%= text_field_tag :dateOfBirth, " ",  
		"data-behaviour" => 'datepicker', 
		placeholder: 'Ej: 25/10/1983'%>


<%= f.label :password, "Clave" %>
<%= f.password_field :password %>

<%= f.label :password_confirmation, "Confirmación" %>
<%= f.password_field :password_confirmation %>